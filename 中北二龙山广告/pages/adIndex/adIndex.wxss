page{
  background: #F3F7F6;
}
.navTop{
  height: 90rpx;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1000;
  width: 100%;
  background:#45AA68;
  color: #ffffff;
  margin-bottom: 20rpx;
}
.navTop-lbs__items{
  position: relative;
  overflow-x: scroll;
  -webkit-overflow-scrolling: touch;
  white-space: nowrap;
  padding: 0rpx 20rpx;
  width: 100%; 
  height: 100rpx;
}
.navTop-list-item{
  display: inline-block;
}
.navTop-list-item__title{
  display: inline-block;
  margin-right: 40rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  line-height: 70rpx;
  height: 70rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  white-space: nowrap;
}
.active-top-border {
  border-bottom: 5rpx solid #ffffff;
}

.item{
  width: 93%;
  height: auto;
  border-radius: 15rpx;
  margin: 25rpx auto;
  background: #FFF;
}
.item:nth-child(2){
  margin-top: 115rpx;
}
.item image{
  display: block;
  width: 100%;
  height: 340rpx;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
}
.item_bottom{
  width: 100%;
  height: 70rpx;
}
.item_bottom_left{
  display: inline-block;
  line-height: 80rpx;
  font-weight: 700;
  font-size: 30rpx;
  padding-left: 20rpx;
}
.item_bottom_right{
  line-height: 80rpx;
  color: #45AA68;
  font-size: 26rpx;
  float: right;
  padding-right: 20rpx;
}
.adNum{
  display: block;
  padding: 0 20rpx;
  line-height: 50rpx;
  color: #8A8B8B;
  font-size: 26rpx;
}
.des{
  display: block;
  padding: 0 20rpx 15rpx;
  line-height: 50rpx;
  color: #8A8B8B;
  font-size: 26rpx;
}

.goTop{
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  position: fixed;
  bottom: 100rpx;
  right: 30rpx;
  background: #000000;
  opacity:0.5;
}
.goTop image{
  width: 60rpx;
  height: 60rpx;
  position: relative;
  top: 15rpx;
  left: 15rpx;
}
.none{
  width: 100%;
  height: auto;
  text-align: center;
  color: #45AA68;
  font-size: 28rpx;
}
.none image{
  display: block;
  width: 666rpx;
  height: 312rpx;
  margin: 380rpx auto 20rpx;
}